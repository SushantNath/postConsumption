<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog id="manufacturingOrderDialog" contentWidth ="85%"
	multiSelect="false"
		noDataText="{i18n>noDataText}"
		search= "handleSearchManufacturing"
		confirm="handleCloseManufacturing"
		cancel="_handleValueHelpCloseManufacturing"
			title="{i18n>manufacturingHeader}"
		items="{
			path : '/Invoices'}">             
		<ColumnListItem>
			<cells>
				<ObjectIdentifier 
				title="{ProductId}" 
				 />
					
					<!--<Text text="{ProductName}" visible="{path:'ProductName', formatter:'.onFormatDate'}"  />-->
					<Text text="{ProductName}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="{i18n>warehouseNum}" />
				</header>
			</Column>
				<Column width="12em">
				<header>
					<Text text="{i18n>manufOrder}" />
				</header>
			</Column>
			
			
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>