<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog id="productSupplyDialog" contentWidth ="85%"
	multiSelect="false"
		noDataText="{i18n>noDataText}"
		search= "handleSearchProdSup"
		confirm="handleCloseProdSup"
		cancel="_handleValueHelpCloseProdSup"
		title="{i18n>prodSupply}"
		items="{
			path : '/Invoices'}">             
		<ColumnListItem>
			<cells>
				<ObjectIdentifier 
				title="{ProductId}" 
				 />
					<Text text="{ProductName}" />	
				
					
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="{i18n>supplyArea}" />
				</header>
			</Column>
				<Column width="12em">
				<header>
					<Text text="{i18n>PSADesc}" />
				</header>
			</Column>
				
			
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>