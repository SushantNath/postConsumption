{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.com.postconsumption.postConsumption",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.38.11"
		},
		"dataSources": {
			"Northwind": {
				"uri": "/destinations/northwind/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			},
			"manage": {
				"uri": "/sap/opu/odata/sap/PP_MPE_OPER_MANAGE",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "sap.com.postconsumption.postConsumption.view.Settings",
			"type": "XML",
			"async": true,
			"id": "Settings"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.com.postconsumption.postConsumption.i18n.i18n"
				}
			},
			"": {
				"dataSource": "Northwind"
			},
			"revenueModel": {
				"dataSource": "Revenue"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sap.com.postconsumption.postConsumption.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteSettings",
					"pattern": "RouteSettings",
					"target": [
						"TargetSettings"
					]
				},
				{
					"name": "postConsumption",
					"pattern": "postConsumption",
					"target": [
						"postConsumption"
					]
				}
			],
			"targets": {
				"TargetSettings": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Settings"
				},
				"postConsumption": {
					"viewType": "XML",
					"viewName": "postConsumption"
				},
				"reversalPosting": {
					"viewType": "XML",
					"viewName": "reversalPosting"
				}
			}
		}
	}
}